<div class="container">
    <br><br>
    <div class="card ">
        <div class="card-content" style="padding-bottom: 0px;">


            <h4>綁定帳號</h4>

            <div class="row">
                <div class="input-field col s12">
                    <input disabled value="" id="input_name" type="text" class="validate">
                    <label for="input_name"></label>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s12">
                    <input id="input_email" type="email" class="validate">
                    <label for="input_email">Email</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s5">
                    <input id="input_year" type="number" class="validate" max="2100" min="1800">
                    <label for="input_year">生日 年</label>
                </div>
                <div class="input-field col s3">
                    <input id="input_month" type="number" class="validate" max="12" min="1">
                    <label for="input_month">月</label>
                </div>
                <div class="input-field col s3">
                    <input id="input_day" type="number" class="validate" max="30" min="1">
                    <label for="input_day" >日</label>
                </div>
            </div>

            <br><br><br><br>
        </div>

    </div>
</div>





